    "@types/debug": "4.1.12",
    "@types/lodash": "4.17.20",
    "@types/mocha": "10.0.10",
    "@types/node": "22.17.2",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "c8": "10.1.3",
    "chai": "4.5.0",
    "chai-as-promised": "8.0.2",
    "cspell": "8.19.4",
