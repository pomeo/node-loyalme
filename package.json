  },
  "scripts": {
    "test": "./node_modules/.bin/c8 mocha",
    "check:spelling": "./node_modules/.bin/cspell --color \"{src,test}/**/*.{ts,md,json,yml}\"",
    "lint": "./node_modules/.bin/eslint \"{src,test}/**/*.{ts}\"",
