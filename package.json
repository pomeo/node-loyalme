    "@types/debug": "4.1.12",
    "@types/lodash": "4.17.0",
    "@types/mocha": "9.1.1",
    "@types/node": "20.12.5",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "c8": "9.1.0",
    "chai": "4.4.1",
    "chai-as-promised": "7.1.1",
    "cspell": "8.6.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-chai-expect": "3.0.0",
